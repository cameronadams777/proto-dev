{
  "name": "proto-dev",
  "version": "1.2.1",
  "description": "An application for demoing ideas offline",
  "productName": "Proto Dev",
  "cordovaId": "org.fiddle.offline.app",
  "capacitorId": "",
  "author": "Cameron Adams <cameronadams777@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "QENV=development quasar dev",
    "dev:electron": "QENV=development quasar dev -m electron",
    "dev:android": "QENV=development quasar dev -m capacitor -T android",
    "dev:ios": "QENV=development quasar dev -m capacitor -T ios",
    "build": "QENV=production quasar build -m pwa",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "firebase": "^7.14.5",
    "lodash": "^4.17.15",
    "quasar": "^1.0.0",
    "vue-codemirror": "^4.0.6",
    "vue-gtag": "^1.6.2",
    "vue-ua": "^1.7.0",
    "vuefire": "^2.2.2",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "@quasar/quasar-app-extension-qenv": "^1.0.1",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "devtron": "^1.4.0",
    "electron": "^8.0.1",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.1.2"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}
